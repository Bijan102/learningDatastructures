package ds.singlylinkedlist;

public class App {

	public static void main(String[] args) {
		//Declare a SinglyLinkedList, 'myList' and instantiate a new SinglyLinkedList object
		SinglyLinkedList myList = new SinglyLinkedList();
		
		//Create a new node and add data to the beginning of myList by calling insertFirst.
		myList.insertFirst(100);
		myList.insertFirst(30);
		myList.insertFirst(60);
		
		//Create a new node and add data to the end of myList by calling insertLast.
		myList.insertLast(40);
		
		//Display the list
		myList.displayList();
	}

}
